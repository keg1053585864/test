<view class="grace-swiper-msg"><view class="{{['grace-swiper-msg-icon',iconClass]}}" style="{{'color:'+(iconColor)+';'}}"></view><swiper vertical="{{vertical}}" autoplay="true" circular="true" interval="{{interval}}"><block wx:for="{{msgs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator url="{{item.url}}" open-type="{{item.opentype}}">{{item.title}}</navigator></swiper-item></block></swiper></view>